<div id="startwrapper">
	<h1>Start a new test</h1>	
	<div class="form-group">
		<label for="type">Test name</label>
		<br>
		<select id="testselector" required>
			<option value="">Select Type</option>
			@model.tests.forEach(function(test)
			{
				<option value="@test.id" data-type="@test.type" data-language="@test.language">@test.name</option>
			})				
		</select>
	</div>			
	<button id="starttest" type="submit" class="btn btn-success">Start test</button>
</div>
	